{
  "expo": {
    "name": "TicktPro",
    "slug": "ticket-pro",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/app-icon.png",
    "userInterfaceStyle": "light",
    "jsEngine": "hermes",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "jsEngine": "hermes",
      "infoPlist": {
        "UIBackgroundModes": [
          "nfc-reader"
        ]
      },
      "supportsTablet": true,
      "bundleIdentifier": "com.ticketpro.ticketpro"
    },
    "android": {
      "permissions": [
        "android.permission.NFC"
      ],
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.mohamedabubaker.ticketpro"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "9ff56627-9124-4751-aa60-69ce0e26cfbd"
      }
    }
  }
}
