{
  "expo": {
    "name": "ticket-pro",
    "slug": "ticket-pro",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "infoPlist": {
        "NFCReaderUsageDescription": "Your description for NFC usage"
      },
      "supportsTablet": true,
      "bundleIdentifier": "ticketpro.com"
    },
    "android": {
      "permissions": [
        "android.permission.NFC"
      ],
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.mohamedabubaker.ticketpro"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
